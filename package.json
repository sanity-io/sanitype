{
  "name": "@sanity/sanitype",
  "version": "0.0.2",
  "description": "Experimental TypeScript-first schemas for Sanity",
  "keywords": [],
  "license": "MIT",
  "author": "Bjørge Næss <bjoerge@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-s clean pkg:build pkg:check",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "pkg:build": "pkg build --strict",
    "pkg:check": "pkg check --strict",
    "pkg:watch": "pkg watch --strict",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .cjs,.js,.ts,.tsx --quiet",
    "test": "vitest run",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -b ./examples --watch",
    "typecheck:examples": "tsc -b ./examples",
    "typecheck:examples:watch": "tsc -b ./examples --watch",
    "test:types": "vitest typecheck --no-watch",
    "test:types:watch": "vitest typecheck",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "example:studio": "vite --config examples/studio/vite.config.ts examples/studio",
    "examples": "run-p example:*",
    "check": "npm-run-all --serial clean --parallel typecheck typecheck:examples lint test test:types",
    "dev": "run-p pkg:watch typecheck:watch typecheck:examples:watch test:watch test:types:watch"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@bjoerge/mutiny": "^0.1.0",
    "@sanity/client": "^6.4.12",
    "@sanity/pkg-utils": "^2.4.9",
    "@sanity/ui": "^1.8.2",
    "@types/object-inspect": "^1.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "mendoza": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "refractor": "^4.8.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": ">= 18"
  }
}
